{
  "name": "minter-console-web",
  "version": "0.5.1",
  "description": "Minter Console Website",
  "author": "shrpne <shrpne@gmail.com>",
  "private": true,
  "scripts": {
    "prod": "npm run production",
    "production": "npm run build",
    "dev": "gulp & nuxt --spa --port=3002 --hostname=localhost",
    "build": "gulp once && nuxt build --spa",
    "start": "nuxt start --spa --port=3002 --hostname=localhost",
    "generate": "nuxt generate",
    "analyze": "gulp once && nuxt build --spa --analyze",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .js,.vue --ignore-path .gitignore --fix .",
    "test": "gulp once && NODE_ENV=test jest --bail --runInBand --verbose",
    "test:debug": "DEBUG=true npm run test",
    "test:debug:skip-build": "DEBUG=true NUXT_SKIP_SELF_BUILD=true npm run test",
    "postinstall": "electron-builder install-app-deps",
    "electron:build": "npm run production && npm run electron:build-x64 && npm run electron:build-x86",
    "electron:build-x64": "electron-builder -mwl --config=./desktop/electron-builder.config.js",
    "electron:build-x86": "electron-builder -w --ia32  --config=./desktop/electron-builder-x86.config.js",
    "electron:build-dir": "electron-builder --config=./desktop/electron-builder.config.js --dir",
    "electron:dev": "electron ./desktop/electron.dev.js",
    "pkg": "npm run prod && npm run pkg:pkg && npm run pkg:zip",
    "pkg:pkg": "pkg . --out-path=tmp/pkg",
    "pkg:zip": "mkdir -p tmp/pkg-zip && cd tmp/pkg && find . -type f -exec zip -D '../pkg-zip/{}.zip' '{}' \\; && cd ../..",
    "precommit": "echo 'Pre-commit checks...' && npm run lint"
  },
  "pre-commit": [
    "precommit"
  ],
  "dependencies": {
    "@material/list": "^3.0.0",
    "@material/menu": "^3.0.0",
    "@material/snackbar": "^3.0.0",
    "@minterteam/footer-links": "^3.0.0",
    "aes-js": "^3.1.2",
    "axios": "^0.19.0",
    "big.js": "^5.2.2",
    "clipbrd": "^1.0.2",
    "cookie": "^0.4.0",
    "core-js": "^3.1.4",
    "custom-event-polyfill": "^1.0.7",
    "date-fns": "^2.0.0-alpha.27",
    "entity-decode": "^2.0.2",
    "js-cookie": "^2.2.0",
    "lodash-es": "^4.17.15",
    "minter-js-org": "^0.3.1",
    "minter-js-sdk": "^0.17.1",
    "minterjs-tx": "^5.1.3",
    "minterjs-util": "0.10.0",
    "minterjs-wallet": "^3.0.4",
    "nuxt": "^2.8.1",
    "nuxt-i18n": "^5.12.7",
    "nuxt-i18n-default": "^0.0.1",
    "nuxt-i18n-preferred": "^0.0.2",
    "pretty-num": "^0.3.0",
    "qr-scanner": "github:shrpne/qr-scanner#build",
    "qrcode.vue": "^1.6.2",
    "v-autosize": "^1.0.3",
    "v-file-input": "^0.1.1",
    "vue-autonumeric": "^1.2.6",
    "vue-imask": "^5.1.7",
    "vue-inline-svg": "^1.1.1",
    "vuelidate": "^0.7.4",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@babel/runtime-corejs3": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "beeper": "^2.0.0",
    "cross-env": "^5.2.0",
    "del": "^5.0.0",
    "dotenv": "^8.0.0",
    "electron": "^5.0.7",
    "electron-builder": "^21.1.1",
    "electron-debug": "^3.0.1",
    "eslint": "^6.0.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-vue": "^5.2.3",
    "faker": "^4.1.0",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "gulp-cache": "^1.1.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-imagemin": "^6.0.0",
    "gulp-less": "^4.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "imagemin-mozjpeg": "^8.0.0",
    "jest": "^24.8.0",
    "jest-environment-node": "^24.8.0",
    "mkdirp": "^0.5.1",
    "pkg": "^4.4.0",
    "postcss-normalize": "^8.0.1",
    "pre-commit": "^1.2.2",
    "puppeteer": "^1.18.1",
    "rimraf": "^2.6.3"
  },
  "main": "desktop/electron.js",
  "bin": "desktop/pkg.js",
  "pkg": {
    "assets": [
      "static",
      "dist/**/*",
      ".nuxt/**/*",
      "node_modules/@nuxtjs/youch/src/resources/**/*"
    ],
    "scripts": [],
    "targets": [
      "macos-x64",
      "linux-x64",
      "win-x64",
      "win-x86"
    ]
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ]
}
